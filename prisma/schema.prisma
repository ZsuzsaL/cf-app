generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model farminfo {
  farmid                     Int       @id @default(autoincrement())
  userid                     Int?
  uniquecffarmid             String?   @unique @db.VarChar(255)
  farmname                   String?   @db.VarChar(255)
  farmmanagerfirstname       String?   @db.VarChar(255)
  farmmanagerlastname        String?   @db.VarChar(255)
  landownerfirstname         String?   @db.VarChar(255)
  landownerlastname          String?   @db.VarChar(255)
  relationshipmanagerorowner String?   @db.VarChar(255)
  email                      String?   @db.VarChar(255)
  farmsize                   String?   @db.VarChar(255)
  address                    String?   @db.VarChar(255)
  postalcode                 String?   @db.VarChar(255)
  farmcoordinatesnorth       String?   @db.VarChar(255)
  farmcoordinateseast        String?   @db.VarChar(255)
  country                    String?   @db.VarChar(255)
  telephone                  String?   @db.VarChar(255)
  creationdate               DateTime? @db.Date
  users                      users?    @relation(fields: [userid], references: [userid], onDelete: NoAction, onUpdate: NoAction)
}

model users {
  userid       Int        @id @default(autoincrement())
  email        String     @unique @db.VarChar(255)
  creationdate DateTime   @default(dbgenerated("CURRENT_DATE")) @db.Date
  farminfo     farminfo[]
}
